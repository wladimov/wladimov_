---
import HeaderLink from './HeaderLink.astro';
import HeaderSocial from './HeaderSocial.astro';
---

<header class="flex justify-between">
	<section>
		<a href="/" aria-label="enlace que redirije al inicio ">
			<img class="w-[144px] h-[25px] " src="/logo.svg" alt="Wladimir Moya @wladimov logo">
		</a>
	</section>
	<section>
		<HeaderSocial />
	</section>
</header>
<style>
	img {
		transition: transform ease .3s;
	}
	img:hover {
		transform: scale(1.1);
		object-fit: contain;
	}
</style>
